{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "semicolon": false,
    "ter-arrow-parens": [true, "as-needed"],
    "variable-name": [true, "allow-pascal-case"],
    "interface-name": false,
    "interface-over-type-literal": false
  },
  "rulesDirectory": []
}
