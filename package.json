{
  "name": "redux-ts",
  "version": "2.3.0",
  "description": "Utils to define redux reducer/action in typescript",
  "main": "lib/index.js",
  "typings": "lib/src/index.d.ts",
  "files": [
    "*.md",
    "dist",
    "LICENSE",
    "lib",
    "src"
  ],
  "repository": "cimdalli/redux-ts",
  "authors": [
    "Okan Ã‡etin"
  ],
  "license": "MIT",
  "scripts": {
    "build:dev": "cross-env NODE_ENV=dev webpack src/index.ts lib/index.js",
    "build:prod": "cross-env NODE_ENV=prod webpack src/index.ts dist/redux-ts.js",
    "build:prod:min": "cross-env NODE_ENV=prod webpack -p src/index.ts dist/redux-ts.min.js",
    "build:test": "cross-env NODE_ENV=test webpack --output-filename lib/specs.js",
    "clean": "rimraf dist lib",
    "build": "npm run build:dev  &&  npm run build:prod  &&  npm run build:prod:min",
    "postinstall": "typings install",
    "prepublish": "npm run clean  &&  npm run build  &&  npm run test",
    "test": "npm run build:test  &&  npm run test:run",
    "test:run": "mocha --require source-map-support/register ./lib/specs.js"
  },
  "tags": [
    "react",
    "redux",
    "typescript",
    "ts",
    "action",
    "reducer",
    "async"
  ],
  "keywords": [
    "react",
    "redux",
    "typescript",
    "ts",
    "action",
    "reducer",
    "async"
  ],
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "ts-helpers": "^1.1.1"
  },
  "devDependencies": {
    "awesome-typescript-loader": "2.1.1",
    "chai": "^3.5.0",
    "cross-env": "^2.0.1",
    "glob": "^7.0.6",
    "mocha": "^2.0.1",
    "rimraf": "^2.5.4",
    "source-map-support": "^0.4.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.3",
    "webpack": "^1.13.2",
    "webpack-node-externals": "^1.3.3"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0",
    "redux": "^2.0.0 || ^3.0.0"
  }
}