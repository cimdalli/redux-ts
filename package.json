  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist lib",
    "build": "npm run ts && webpack lib/index.js dist/redux-ts.js",
    "start": "npm run clean && npm run build",
    "postinstall": "typings install",
    "prepublish": "npm run start",
    "mocha": "mocha --require ./lib/test/test/index.js",
    "test": "npm run ts:test && npm run mocha",
    "ts:src": "npm run tsc",
    "ts:test": "npm run tsc -- --target es5 --outDir lib/test test/index.ts typings/index.d.ts",
    "ts": "npm run ts:src && npm run ts:test",
    "tsc": "tsc"
  },
  "tags": [
    "react",
    "redux",
    "typesctipt",
    "action",
    "reducer",
