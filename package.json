    "typesctipt",
    "action",
    "reducer",
    "decorator"
  ],
  "dependencies": {
    "lodash": "^4.15.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "ts-helpers": "^1.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "karma": "^1.2.0",
    "mocha": "^3.0.2",
    "rimraf": "^2.3.4",
    "ts-loader": "^0.8.1",
    "typescript": "^1.8.10",
