  "scripts": {
    "clean": "rimraf dist lib",
    "dist": "webpack src/index.ts dist/redux-ts.js  &&  webpack -p src/index.ts dist/redux-ts.min.js",
    "specs:build": "webpack --config test/webpack.config.js",
    "specs:run": "mocha ./lib/specs.js",
